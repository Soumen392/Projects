<% include ./partials/header.ejs %>
<div class="w-full min-h-screen bg-zinc-700 px-10 py-5 text-white">
    <h1 class="text-xl">Update your Profile</h1>
    <hr class="opacity-10 mt-3">
    <form action="/update" method="post" enctype="multipart/form-data">
        <input class="text-black block w-1/3 px-3 py-2 border-2 rounded-lg mt-3 " type="text" placeholder="fullname" name="fullname">
        <input class="text-black block w-1/3 px-3 py-2 border-2 rounded-lg mt-3 " type="text" placeholder="address" name="address">
        <input class="text-black block w-1/3 px-3 py-2 border-2 rounded-lg mt-3 " type="number" placeholder="contact" name="contact">
        <input class="text-black block w-1/3 px-3 py-2  rounded-lg mt-3 bg-orange-400" type="submit" value="Update">
    </form>
</div>
<% include ./partials/footer.ejs %>